  "scripts": {
    "analyze": "npm run build:stats && npm run start:analyzer",
    "build": "NODE_ENV=production node scripts/build.js",
    "build:live": "NODE_ENV=production LIVE_GA=true node scripts/build.js",
    "build:lite": "NODE_ENV=production LITE_BUILD=true node scripts/build.js",
    "build:nomap": "NODE_ENV=production OMIT_SOURCEMAP=true node scripts/build.js",
    "build:with-html": "NODE_ENV=production GEN_HTML=yas node scripts/build.js",
    "build:stats": "NODE_ENV=production LITE_BUILD=true MUTE_PACK=true webpack --config config/webpack.config.js/client.prod.js --json > build/client/static/bundle-stats.json",
    "start": "NODE_ENV=development node scripts/start.js",
    "start:analyzer": "webpack-bundle-analyzer build/client/static/bundle-stats.json",
    "start:prod-test": "NODE_ENV=production node build/server/server.js",
    "logs:all": "node src/shared/logging/query.js",
    "init:db": "node config/initdb.js",
    "init:clean": "rm  ./images/raw/1.png ./images/webp/1.png.webp ./images/brotli/1.png.webp.br ./images/raw/2.png ./images/webp/2.png.webp ./images/brotli/2.png.webp.br ./images/raw/3.png ./images/webp/3.png.webp ./images/brotli/3.png.webp.br ./images/raw/4.png ./images/webp/4.png.webp ./images/brotli/4.png.webp.br ./images/raw/5.png ./images/webp/5.png.webp ./images/brotli/5.png.webp.br"
